{
	"name": "@visual-analytics-components/docsite",
	"private": true,
	"version": "0.3.0",
	"scripts": {
		"clean": "gatsby clean",
		"start": "gatsby develop -H 0.0.0.0",
		"start:docsite": "yarn start",
		"bundle_gatsby": "gatsby build --prefix-paths",
		"copy_embeddings_explorer": "shx cp -r ../app-embeddings-explorer/build/ ./static/embeddings-explorer",
		"copy_storybook": "shx cp -r ../app-stories/storybook-static/ ./static/storybook",
		"create_apidocs_folder": "shx mkdir -p static/apidocs",
		"apidoc:vac": "shx cp -r ../lib-vac/dist/docs static/apidocs/vac",
		"apidoc:manager": "shx cp -r ../lib-manager/dist/docs static/apidocs/manager",
		"apidoc:messages": "shx cp -r ../lib-messages/dist/docs static/apidocs/messages",
		"apidoc:types": "shx cp -r ../lib-types/dist/docs static/apidocs/types",
		"apidoc:abstract-dataset": "shx cp -r ../dataset-abstract/dist/docs static/apidocs/abstract-dataset",
		"apidoc:mapped-dataset": "shx cp -r ../dataset-mapped/dist/docs static/apidocs/mapped-dataset",
		"apidoc:plain-object-dataset": "shx cp -r ../dataset-plain-object/dist/docs static/apidocs/plain-object-dataset",
		"apidoc:datatable-component": "shx cp -r ../component-datatable/dist/docs static/apidocs/datatable-component",
		"apidoc:filterchip-component": "shx cp -r ../component-filterchip/dist/docs static/apidocs/filterchip-component",
		"apidoc:graph-component": "shx cp -r ../component-graph/dist/docs static/apidocs/graph-component",
		"apidoc:ranklist-component": "shx cp -r ../component-ranklist/dist/docs static/apidocs/ranklist-component",
		"apidoc:search-component": "shx cp -r ../component-search/dist/docs static/apidocs/search-component",
		"apidoc:title-component": "shx cp -r ../component-title/dist/docs static/apidocs/title-component",
		"copy_apidocs": "npm-run-all -s create_apidocs_folder -p 'apidoc:*'",
		"copy_static_assets": "run-p copy_apidocs copy_embeddings_explorer copy_storybook",
		"bundle": "run-s copy_static_assets bundle_gatsby"
	},
	"author": "Chris Trevino <chtrevin@microsoft.com>",
	"contributors": [],
	"dependencies": {
		"@mdx-js/mdx": "^1.6.18",
		"@mdx-js/react": "^1.6.18",
		"@mikaelkristiansson/domready": "^1.0.10",
		"@reach/router": "^1.3.4",
		"@types/debug": "^4.1.5",
		"@types/gsap": "^1.20.2",
		"@types/react": "^16.9.53",
		"@types/react-dom": "^16.9.8",
		"@types/react-helmet": "^6.1.0",
		"@types/react-tabs": "^2.3.2",
		"@types/styled-components": "^5.1.4",
		"@visual-analytics-components/datatable-component": "workspace:packages/component-datatable",
		"@visual-analytics-components/embeddings-explorer": "workspace:packages/app-embeddings-explorer",
		"@visual-analytics-components/filterchip-component": "workspace:packages/component-filterchip",
		"@visual-analytics-components/graph-component": "workspace:packages/component-graph",
		"@visual-analytics-components/ranklist-component": "workspace:packages/component-ranklist",
		"@visual-analytics-components/search-component": "workspace:packages/component-search",
		"@visual-analytics-components/title-component": "workspace:packages/component-title",
		"babel-plugin-styled-components": "^1.11.1",
		"debug": "^4.2.0",
		"event-source-polyfill": "^1.0.21",
		"gatsby": "^2.24.83",
		"gatsby-legacy-polyfills": "^0.0.5",
		"gatsby-link": "^2.4.16",
		"gatsby-plugin-mdx": "^1.2.49",
		"gatsby-plugin-react-helmet": "^3.3.14",
		"gatsby-plugin-styled-components": "^3.3.14",
		"gatsby-plugin-typescript": "^2.4.23",
		"gatsby-plugin-typography": "^2.5.13",
		"gatsby-react-router-scroll": "^3.0.15",
		"gatsby-remark-autolink-headers": "^2.3.15",
		"gatsby-remark-copy-linked-files": "^2.3.19",
		"gatsby-remark-images": "^3.3.37",
		"gatsby-remark-prismjs": "^3.5.16",
		"gatsby-source-filesystem": "^2.3.35",
		"gatsby-transformer-remark": "^2.8.44",
		"gsap": "^3.5.1",
		"mitt": "^2.1.0",
		"prismjs": "^1.22.0",
		"prop-types": "^15.7.2",
		"query-string": "^6.13.6",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-error-overlay": "^6.0.7",
		"react-helmet": "^6.1.0",
		"react-icons": "^3.11.0",
		"react-tabs": "^3.1.1",
		"react-typography": "^0.16.19",
		"react-use-dimensions": "^1.2.1",
		"rehype-react": "^5.0.1",
		"shallow-compare": "^1.2.2",
		"styled-components": "^5.2.0",
		"typography": "^0.16.19",
		"typography-theme-github": "^0.16.19",
		"vfile": "^4.2.0",
		"vfile-message": "^2.0.4"
	}
}
