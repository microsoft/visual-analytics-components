{
	"name": "@visual-analytics-components/docsite",
	"private": true,
	"version": "0.3.0",
	"scripts": {
		"clean": "gatsby clean",
		"start": "gatsby develop -H 0.0.0.0",
		"bundle_gatsby": "gatsby build --prefix-paths",
		"copy_embeddings_explorer": "shx cp -r ../embeddings-explorer/build/ ./static/embeddings-explorer",
		"copy_storybook": "shx cp -r ../stories/storybook-static/ ./static/storybook",
		"create_apidocs_folder": "shx mkdir -p static/apidocs",
		"apidoc:vac": "shx cp -r ../../authoring/vac/dist/docs static/apidocs/vac",
		"apidoc:manager": "shx cp -r ../../authoring/manager/dist/docs static/apidocs/manager",
		"apidoc:messages": "shx cp -r ../../authoring/messages/dist/docs static/apidocs/messages",
		"apidoc:types": "shx cp -r ../../authoring/types/dist/docs static/apidocs/types",
		"apidoc:abstract-dataset": "shx cp -r ../../datasets/abstract-dataset/dist/docs static/apidocs/abstract-dataset",
		"apidoc:kruda-dataset": "shx cp -r ../../datasets/kruda-dataset/dist/docs static/apidocs/kruda-dataset",
		"apidoc:mapped-dataset": "shx cp -r ../../datasets/mapped-dataset/dist/docs static/apidocs/mapped-dataset",
		"apidoc:plain-object-dataset": "shx cp -r ../../datasets/plain-object-dataset/dist/docs static/apidocs/plain-object-dataset",
		"apidoc:datatable-component": "shx cp -r ../../components/datatable/dist/docs static/apidocs/datatable-component",
		"apidoc:filterchip-component": "shx cp -r ../../components/filterchip/dist/docs static/apidocs/filterchip-component",
		"apidoc:graph-component": "shx cp -r ../../components/graph/dist/docs static/apidocs/graph-component",
		"apidoc:ranklist-component": "shx cp -r ../../components/ranklist/dist/docs static/apidocs/ranklist-component",
		"apidoc:search-component": "shx cp -r ../../components/search/dist/docs static/apidocs/search-component",
		"apidoc:title-component": "shx cp -r ../../components/title/dist/docs static/apidocs/title-component",
		"copy_apidocs": "npm-run-all -s create_apidocs_folder -p 'apidoc:*'",
		"copy_static_assets": "run-p copy_apidocs copy_embeddings_explorer copy_storybook",
		"bundle": "run-s copy_static_assets bundle_gatsby"
	},
	"author": "Chris Trevino <chtrevin@microsoft.com>",
	"contributors": [],
	"dependencies": {
		"@mdx-js/mdx": "^1.6.16",
		"@mdx-js/react": "^1.6.16",
		"@mikaelkristiansson/domready": "^1.0.10",
		"@reach/router": "^1.3.4",
		"@types/debug": "^4.1.5",
		"@visual-analytics-components/embeddings-explorer": "^0.3.0",
		"debug": "^4.1.1",
		"event-source-polyfill": "^1.0.17",
		"gatsby": "^2.24.11",
		"gatsby-legacy-polyfills": "^0.0.4",
		"gatsby-link": "^2.4.13",
		"gatsby-plugin-mdx": "^1.2.32",
		"gatsby-plugin-react-helmet": "^3.3.10",
		"gatsby-plugin-styled-components": "^3.3.10",
		"gatsby-plugin-typescript": "^2.4.16",
		"gatsby-plugin-typography": "^2.5.10",
		"gatsby-react-router-scroll": "^3.0.12",
		"gatsby-remark-autolink-headers": "^2.3.11",
		"gatsby-remark-copy-linked-files": "^2.3.12",
		"gatsby-remark-images": "^3.3.24",
		"gatsby-remark-prismjs": "^3.5.10",
		"gatsby-source-filesystem": "^2.3.23",
		"gatsby-transformer-remark": "^2.8.27",
		"gsap": "^3.4.2",
		"mitt": "^2.1.0",
		"prismjs": "^1.21.0",
		"prop-types": "^15.7.2",
		"query-string": "^6.13.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-error-overlay": "^6.0.7",
		"react-helmet": "^6.1.0",
		"react-icons": "^3.10.0",
		"react-tabs": "^3.1.1",
		"react-typography": "^0.16.19",
		"react-use-dimensions": "^1.2.1",
		"rehype-react": "^5.0.1",
		"shallow-compare": "^1.2.2",
		"styled-components": "^5.1.1",
		"typography": "^0.16.19",
		"typography-theme-github": "^0.16.19",
		"vfile": "^4.2.0",
		"vfile-message": "^2.0.4"
	},
	"devDependencies": {
		"@types/gsap": "^1.20.2",
		"@types/react": "^16.9.44",
		"@types/react-dom": "^16.9.8",
		"@types/react-helmet": "^6.0.0",
		"@types/react-tabs": "^2.3.2",
		"@types/styled-components": "^5.1.2",
		"@types/vfile": "^4.0.0",
		"@types/vfile-message": "^2.0.0",
		"@visual-analytics-components/datatable-component": "^0.3.0",
		"@visual-analytics-components/filterchip-component": "^0.3.0",
		"@visual-analytics-components/graph-component": "^0.3.0",
		"@visual-analytics-components/ranklist-component": "^0.3.0",
		"@visual-analytics-components/search-component": "^0.3.0",
		"@visual-analytics-components/title-component": "^0.3.0"
	}
}
